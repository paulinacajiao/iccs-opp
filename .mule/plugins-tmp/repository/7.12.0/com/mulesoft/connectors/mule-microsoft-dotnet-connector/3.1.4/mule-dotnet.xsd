          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="path"></xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="resourceFolder"></xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
